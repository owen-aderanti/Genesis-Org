        <!--###### MODAL BOX Start######--> 
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open slds-modal_large">
            <div class="slds-modal__container">
                <!-- ###### MODAL BOX HEADER Start ######-->
                <header class="slds-modal__header">
                    <lightning:buttonIcon iconName="utility:close"
                                          onclick="{! c.closeModel }"
                                          alternativeText="close"
                                          variant="bare-inverse"
                                          class="slds-modal__close"/>
                    <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">Custom Batch Invoice</h2>
                </header>
                <!--###### MODAL BOX BODY Part Start######-->
                <div class="slds-modal__content slds-p-around_medium bigBox" id="modal-content-id-1">
                    <!-- Line 1 -->
                    <div>
                        
                        <div id="accountDrop" class="customDropdowns">
                            <aura:If isTrue="{! v.showLabAcc}">
                                <label class="slds-form-element__label" for="combobox-id-1">Account</label>
                            </aura:If>
                            
                            <div class="slds-form-element__control checker">
                                <div class="slds-combobox_container">
                                    <div class="slds-combobox slds-dropdown-trigger slds-dropdown-trigger_click " aura:id="resultsDivAcc" aria-expanded="true" aria-haspopup="listbox" role="combobox">
                                        <div class="slds-combobox__form-element slds-input-has-icon slds-input-has-icon_right" role="none">
                                            <div aura:id="lookup-pill" class="{! 'slds-pill-container ' + if(v.selectedAccRecord == '','slds-hide','') }">
                                                <!-- Selected Value Pill -->
                                                <lightning:pill class="fullWidth" label="{!v.selectedAccRecord.label}" name="{!v.selectedAccRecord.value}" onremove="{! c.removeItem }">
                                                    <aura:set attribute="media">
                                                        <lightning:icon iconName="{!v.iconNameAcc}" size="x-small" alternativeText="icon"/>
                                                    </aura:set>
                                                </lightning:pill>
                                            </div>
                                            <div aura:id="lookupField" class="{! if(v.selectedAccRecord == '','slds-show','slds-hide') }">
                                                <lightning:input aura:id="inputLookupAcc" id="combobox-id-1" label="Account" role="textbox" class="inputBox" placeholder="{!v.placeholderAcc}" onkeyup="{!c.searchRecords}" value="{!v.searchAccString}" onblur="{!c.blurEvent}" aria-controls="listbox-id-1" autoComplete="off" required="true" />
                                            </div>
                                        </div>
                                        <!-- Dropdown List -->
                                        <div id="listbox-id-1" class="slds-dropdown slds-dropdown_length-5 slds-dropdown_fluid" role="listbox" style="{! 'max-height:' + (8 + (v.recordCount * 40)) + 'px' }">
                                            <ul class="slds-listbox slds-listbox_vertical recordListBox" role="presentation">
                                                <aura:if isTrue="{! v.accMessage == '' }" >
                                                    <!-- To display Drop down List -->
                                                    <aura:iteration items="{!v.accounts}" var="rec" >
                                                        <li id="{!rec.value}" class="slds-listbox__item" onmousedown="{!c.selectItem}">
                                                            <div class="slds-media slds-listbox__option_entity">
                                                                <span>
                                                                    <lightning:icon iconName="{!v.iconNameAcc}" alternativeText="icon"></lightning:icon>
                                                                </span>
                                                                <span class="verticalAlign">
                                                                    <span class="slds-truncate">{!rec.label}</span>
                                                                </span>
                                                            </div>
                                                        </li>
                                                    </aura:iteration>
                                                    <!-- To display Error Message -->
                                                    <aura:set attribute="else">
                                                        <li class="slds-listbox__item">
                                                            <div id="option1" class="slds-media slds-listbox__option slds-listbox__option_entity slds-listbox__option_has-meta">
